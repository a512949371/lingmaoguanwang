page {
    width: 100%;
    height: 100%;
    overflow: hidden;
    overflow-y: auto;
}

.r-detail {
    width: 690rpx;
    height: 100%;
    padding: 30rpx;
    padding-top: 60rpx;
    margin-bottom: 100rpx;
}

.detail-info-title {
    display: inline-block;
}

.info-title-type {
    font-size: 42rpx;
    color: #494949;
    font-weight: bold;
    display: inline-block;
    width: 500rpx;
}

.info-title-income {
    font-size: 36rpx;
    color: #FD5C60;
    display: inline-block;
    text-align: right;
    width: 190rpx;
    font-weight: bold;
}

.detail-info-desc {
    display: inline-block;
    margin-top: 20rpx;
}

.detail-info-desc .info-desc {
    display: inline-block;
    padding: 0 5px;
    text-align: left;
    font-size: 28rpx;
    color: #494949;
    margin-right: 10rpx;
}

.info-desc image {
    margin-right: 10rpx;
}

.detail-info-more {
    margin-top: 20rpx;
    margin-bottom: 40rpx;
}

.info-more-title {
    font-size: 32rpx;
    color: #494949;
}

.info-more-content {
    margin-top: 20rpx;
    width: 690rpx;
    height: 100%;
    display: block;
}

.map-info {
    width: 695rpx;
    height: 420rpx;
}

.map-info map {
    display: block;
    width: 100%;
    height: 400rpx;
}

.detail-info-att {
    margin-top: 20rpx;
    display: block;
    width: 100%;
    font-size: 32rpx;
    color: #FD5C60;
}

.info-att {}

.info-att-text {
    height: 50rpx;
    line-height: 50rpx;
    width: 100%;
    display: block;
    color: #242424;
    font-size: 24rpx;
    padding-bottom: 40rpx;
}