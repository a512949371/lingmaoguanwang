<!--pages/contactus/contactus.wxml-->
<view class='pd40'>
  <view class='top-box flex-box-column'>
    <image src='../../images/index-chose.png' class='logo'></image>
    <view class='name'>零猫科技</view>
    <view class='desc'>首家创新型互联网科技公司</view>
  </view>
  <view class='flex-box-center'>
    <view class='more' bindtap="profileMore">了解更多</view>
  </view>
  <view class='add-info-box'>
    <view class="reserve-title flex-box">
      <view class='verticalline'></view>
      <text>喵家地址</text>
    </view>
    <view class='add-info flex-box'>
      <view class='desc'>昆明市官渡区银海樱花语幸福广场商务写 字楼A座二楼
      </view>
      <view class='flex-box-column btn-box' bindtap='Openmap'>
        <image src='../../images/icon_add.png' class='img'></image>
        <view class='go-btn'>去这里</view>
      </view>
    </view>
    <view class='map-box pt40' bindtap='Openmap'>
      <!-- <map id="map" longitude="102.747751" latitude="24.957532" scale="18" controls="{{controls}}" markers="{{markers}}" show-location style="width: 100%; height: 150px;" enable-zoom='{{false}}' enable-scroll='{{false}}'></map> -->
      <image src='../../images/map.png'> </image>
    </view>
  </view>
  <view class='conus-box'>
    <view class="reserve-title flex-box">
      <view class='verticalline'></view>
      <text>联系我们</text>
    </view>
    <view class='pt20'>
    <view class='box ' bindtap='Callme' style='padding-top:0'>手机号码：136423848435</view>
    <view class='box'>邮箱：fhgghh@sdf.cn</view>
    </view>
  </view>
  <view class='fixed-call'>
     <image src='../../images/call.png' bindtap='Callme'></image>
    </view>
</view>